[sub_resource type="GDScript" id="GDScript_cfhva"]
script/source = "extends Area2D

var collided_timer : = 1000.0
var current_timer : = 0.0

func _ready():
	$Line2D.set_as_top_level(true)
	$CollisionShape2D.shape = CircleShape2D.new()
	$CollisionShape2D.shape.radius = randf() * 128 + 1
	
func _physics_process(delta):
	$CollisionShape2D.debug_color.r = current_timer/collided_timer
	collided_timer -= 1 
	$Line2D.add_point(global_position)
	if $Line2D.points.size() > 10:
		$Line2D.remove_point(0)
	
	var overlapping_areas : Array = get_overlapping_areas()
	if overlapping_areas.size() > 0:
		for other_area in overlapping_areas:
			if other_area.name == \"Gravity\":
				continue
			var my_radius : float = $CollisionShape2D.shape.radius
			var their_radius : float = other_area.get_node(\"CollisionShape2D\").shape.radius
			
			var my_body : = get_parent()
			var their_body : Node2D = other_area.get_parent()
			
			var distance_between : float = their_body.global_position.distance_to(my_body.global_position)
			var direction_to : Vector2 = their_body.global_position.direction_to(my_body.global_position)
			
			my_body.global_position = their_body.global_position + (direction_to * (my_radius + their_radius + 0.1))
			current_timer = collided_timer
"

