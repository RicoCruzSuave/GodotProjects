[sub_resource type="GDScript" id="GDScript_0ax2j"]
script/source = "extends MFM

func _ready():
	super._ready()
	for x in world_size.x:
		for y in world_size.y:
			var new_cell : = Cell.new()
			new_cell.id = 7
			world[x][y].content = new_cell
			draw_cell(Vector2i(x,y))

func _input(event):
	if Input.is_action_just_pressed(\"left_click\"):
		var coords : = local_to_map(to_local(get_global_mouse_position()))
		if is_space_valid(coords):
			world[coords.x][coords.y].content = Cell.new()
			world[coords.x][coords.y].content.randomize_genome()			
			#world[coords.x][coords.y].content.id = randi_range(0,3)
			draw_cell(coords)
		
	if Input.is_action_just_pressed(\"right_click\"):
		var coords : = local_to_map(to_local(get_global_mouse_position()))
		if is_in_bounds(coords):
			world[coords.x][coords.y].content.update(neighbors[coords.x][coords.y])
			draw_cell(coords)
			draw_cell(coords + Vector2i.RIGHT)
			draw_cell(coords + Vector2i.LEFT)
			
	if Input.is_action_just_pressed(\"ui_accept\"):
		update_cells()
		
	if Input.is_action_just_pressed(\"ui_down\"):
		for x in world_size.x:
			for y in world_size.y:
				world[x][y].content.id = randi_range(0,3)
				draw_cell(Vector2i(x,y))
				
class Cell:
	enum GENE {
		LEFT,
		RIGHT,
		UP,
		DOWN,
		SELF,
		MAX
	}
	enum CELL_TYPE {
		SEED,
		WOOD,
		LEAF,
		ROOT,
		ANTENNA
	}
	#[L, U, R, D, S]
	var genome : = [
		[0, 7, 7, 7, 7], #Turn Left
		[7, 7, 1, 7, 7], #Turn right
		[7, 2, 7, 7, 7], #Go up
		[7, 7, 7, 3, 7], #Go down
		[7, 7, 7, 7, 7], #Dont move
	]
	var id : = CELL_TYPE.SEED #0 - Seed, 1 - wood, 2 - leaf, 3 - root, 4 - antenna
	var genome_index : = 0
	var energy : = 100
	
	func randomize_genome():
		var new_genome : = []
		for _j in genome.size():
			var new_array = []
			for _i in range(GENE.MAX):
				var rand : = randi_range(-1, GENE.MAX - 1)
				if rand == -1:
					rand = 7
				new_array.append(rand)
			new_genome.append(new_array)
		genome = new_genome
	
	func get_instructions() -> Array:
		if id == 7:
			return genome[4]
		var current_instruction : Array = genome[id] 
		id = 4
		return current_instruction

	func update(neighbors: Dictionary):
		if id == 7:
			return
		#energy -= 1
		if energy < 0:
			die()
		match id:
			CELL_TYPE.SEED:
				if genome_index == 7:
					return
				var instructions : Array = genome[genome_index]
				#if neighbors[Vector2i.LEFT].content == null:
					#var new_cell : = Cell.new()
					#new_cell.energy = energy - 1
					#new_cell.id = CELL_TYPE.SEED
					#new_cell.genome_index = instructions[GENE.LEFT]
				#el
				if neighbors[Vector2i.LEFT] != null and neighbors[Vector2i.LEFT].content.id == 7:
					neighbors[Vector2i.LEFT].content.id = CELL_TYPE.SEED
					neighbors[Vector2i.LEFT].content.energy = energy - 1 
					neighbors[Vector2i.LEFT].content.genome_index = instructions[GENE.LEFT]
				if neighbors[Vector2i.RIGHT] != null and neighbors[Vector2i.RIGHT].content.id == 7:
					neighbors[Vector2i.RIGHT].content.id = CELL_TYPE.SEED
					neighbors[Vector2i.RIGHT].content.energy = energy - 1 
					neighbors[Vector2i.RIGHT].content.genome_index = instructions[GENE.LEFT]
				if neighbors[Vector2i.UP] != null and neighbors[Vector2i.UP].content.id == 7:
					neighbors[Vector2i.UP].content.id = CELL_TYPE.SEED
					neighbors[Vector2i.UP].content.energy = energy - 1 
					neighbors[Vector2i.UP].content.genome_index = instructions[GENE.LEFT]
				if neighbors[Vector2i.DOWN] != null and neighbors[Vector2i.DOWN].content.id == 7:
					neighbors[Vector2i.DOWN].content.id = CELL_TYPE.SEED
					neighbors[Vector2i.DOWN].content.energy = energy - 1 
					neighbors[Vector2i.DOWN].content.genome_index = instructions[GENE.LEFT]
				neighbors[Vector2i.ZERO].content.id = instructions[GENE.SELF]
					
			CELL_TYPE.WOOD:
				pass
			CELL_TYPE.LEAF:
				energy += 5
				#share_energy()
			CELL_TYPE.ROOT:
				pass
			CELL_TYPE.ANTENNA:
				pass
			7:
				print_debug(\"It dead \")
			_:
				print_debug(\"Aliens\")

	func swap_cells(neighbors : Dictionary, dir : Vector2i):
		var self_ref = neighbors[Vector2i.ZERO].content 
		var neighbor_ref = neighbors[dir].content
		neighbors[Vector2i.ZERO].content = neighbor_ref
		neighbors[dir].content = self_ref
		
	func die():
		id = 7
"

[sub_resource type="GDScript" id="GDScript_aljtg"]
script/source = "extends MFM

var p_image : = Image.create(
	world_size.x, 
	world_size.y,
	false,
	Image.FORMAT_RGBAF
)
var pheromone_texture : = ImageTexture.create_from_image(p_image)
@onready var pheromone_sprite : = $Sprite2D

func _ready():
	super._ready()
	for x in world_size.x:
		for y in world_size.y:
			var space : Space = world[x][y]
			var initial_amount : = 0.0
			space.environment[\"pheromones\"] = initial_amount
			p_image.set_pixel(x,y,Color(initial_amount, 0.0, 0.0, 1.0))
	pheromone_texture.set_image(p_image)
	pheromone_sprite.texture = pheromone_texture
	
func _input(event):
	if Input.is_action_just_pressed(\"left_click\"):
		var coords : = local_to_map(to_local(get_global_mouse_position()))
		if is_in_bounds(coords):
			world[coords.x][coords.y].environment[\"pheromones\"] = 10.0
			draw_cell(coords)
	if Input.is_action_just_pressed(\"right_click\"):
		var coords : = local_to_map(to_local(get_global_mouse_position()))
		if is_in_bounds(coords):
			world[coords.x][coords.y].content = Ant.new()
			draw_cell(coords)
	if Input.is_action_just_pressed(\"ui_accept\"):
		update_cells()

func _process(delta):
	var cell : = Vector2i(
		randi_range(0, world_size.x - 1),
		randi_range(0, world_size.y - 1),
	)
	var avg_pheromone : = 0.0
	for x in range(-1, 1):
		for y in range(-1, 1):
			var offset : = Vector2i(x,y)
			
	var current_pheromone : float = world[cell.x][cell.y].environment[\"pheromones\"]
	current_pheromone -= 0.01
	var background_color : = Color.BLACK
	background_color.r = current_pheromone
	p_image.set_pixelv(cell, background_color)
	pheromone_sprite.texture.update(p_image)
	
class Ant:
	var id : = 0
	var species_id : = 0
	var dir : = Vector2.RIGHT
	
	func update(neighbors : Dictionary):
		var dir_array : = [
			dir.rotated(-TAU/4.0), 
			dir.rotated(TAU/4.0),
			dir,
		]
		var highest_pheromone : = 0.0
		for i in dir_array:
			var current_dir : = Vector2i(i)
			if neighbors[current_dir].environment[\"pheromones\"] > highest_pheromone:
				dir = current_dir
"

